
@{
    ViewBag.Title = "Settings";
    Layout = "~/Views/Shared/BranchSideNav.cshtml";
}
<link href="~/Content/adminstyles/select2-min.css" rel="stylesheet" />
<link href="~/Content/adminstyles/Settings.css" rel="stylesheet" />
<div id="page-wrapper" style="padding-top:5%">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Settings</h1>
        </div>
        <h3 class="page-header textalline-center">Create New Line</h3>
    </div>
    <div class="row select-margins">
        <div class="col">
            <label for="selectdepartment"> Department</label>
            <select style=" width:20%" class="js-example-placeholder-single" multiple="multiple" id="selectdepartment" name="selectdepartment">
                <option>Assembly</option>
                <option>Machine Shop</option>
            </select>
        </div>
    </div>
    <div class="row select-margins">
        <div class="col">
            <label for="selectline"> Line</label>
            <select style=" width:20%" class="js-example-placeholder-single" multiple="multiple" id="selectline" name="selectline">
                <option>line 1</option>
                <option>line 2</option>
                <option>line 3</option>
            </select>
        </div>
    </div>

    <button class='btn btn-success' data-target='#modal1' data-toggle='modal'>Show Modal</button>


</div>

<div class="modal" data-keyboard="false" id="modal1" tabindex="-1" style="color:black; margin-top:10px;background-color: transparent;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="background-color: #49aa27; color: white;">

                <h4 class="modal-title" style="margin:auto;">MODAL NAME</h4>
                <button class="close" data-dismiss="modal">&times;</button>
            </div>

            <div class="modal-body" style="display:flex;">
                <p>Modal body</p>
            </div>
        </div>
    </div>
</div>
<script src="JQUERY/jquery.min.js"></script>
<script src="bootstrap/js/bootstrap.min.js"></script>
<script src="~/Scripts/AdminScripts/jquery.js"></script>
<script src="~/Scripts/AdminScripts/select2-min.js"></script>
<script src="~/Scripts/AdminScripts/settings.js"></script>

